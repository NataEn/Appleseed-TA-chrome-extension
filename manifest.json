{
    "name": "Appleseed Work Space",
    "version": "1.0",
    "description": "Appleseed platforms and links",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/logo16.png",
            "32": "images/logo32.png",
            "48": "images/logo48.png",
            "128": "images/logo128.png",
            "136": "images/logo136.png"
          }
    },
    "permissions":["activeTab","declarativeContent","storage"],
    "icons": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png",
        "136": "images/logo136.png"
      },
    "manifest_version": 2
}